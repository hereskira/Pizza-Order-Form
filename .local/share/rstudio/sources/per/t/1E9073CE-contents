# Load necessary libraries
if (!require("dplyr")) install.packages("dplyr", dependencies = TRUE)

library(dplyr)

# Load the data
data <- read.csv("RISKFACTORSANDACCESSTOCARE1.csv")

# Define the columns to check for -1111.1
columns_to_check <- c("No_Exercise", "Few_Fruit_Veg", "Obesity", 
                      "High_Blood_Pres", "Smoker", "Diabetes")

# Filter out rows where any of the specified columns have the value -1111.1
filtered_data <- data %>%
  filter(across(all_of(columns_to_check), ~ . != -1111.1))

# Display the cleaned dataset
print(filtered_data)

# Save the cleaned dataset to a new file
write.csv(filtered_data, "Filtered_RISKFACTORSANDACCESSTOCARE1.csv", row.names = FALSE)

cat("Filtered dataset saved as 'Filtered_RISKFACTORSANDACCESSTOCARE1.csv'")

data <- read.csv("Filtered_RISKFACTORSANDACCESSTOCARE1.csv")
